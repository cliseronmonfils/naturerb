<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Recette">

<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/libelle"
        android:layout_width="464dp"
        android:layout_height="47dp"
        android:layout_marginTop="88dp"
        android:background="#4472C4"
        android:gravity="center"
        android:text="*Symptome*"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="#FFFFFF"
        android:textSize="19sp"
        app:fontFamily="sans-serif"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</LinearLayout>

    <ImageView
        android:id="@+id/logo-nerb"
        android:layout_width="68dp"
        android:layout_height="54dp"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logonerb" />

    <ImageButton
        android:id="@+id/logo_menu2"
        android:layout_width="38dp"
        android:layout_height="34dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:background="@null"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_menu" />

    <ImageButton
        android:id="@+id/logo_acceuil2"
        android:layout_width="37dp"
        android:layout_height="35dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="83dp"
        android:layout_marginRight="83dp"
        android:background="@null"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_accueil" />

    <EditText
        android:id="@+id/editText3"
        android:layout_width="383dp"
        android:layout_height="729dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="240dp"
        android:layout_marginEnd="12dp"
        android:layout_marginRight="12dp"
        android:clickable="false"
        android:cursorVisible="false"
        android:ems="10"
        android:focusable="false"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        android:longClickable="false"
        android:text="Lorem ipsum dolor sit amet, consectetur adipiscing elit. Phasellus venenatis nibh eu odio sagittis, ultricies iaculis velit iaculis. Fusce mattis id sapien vel hendrerit. Nullam eget leo justo. Nulla sed sodales lacus. Curabitur at ligula leo. Aenean non erat sed nibh eleifend viverra et et lorem. Etiam ultricies erat eget accumsan laoreet. Etiam eu nibh elit. Vivamus aliquam laoreet risus sed fringilla. Pellentesque nibh nisl, convallis a elit in, luctus congue lorem. Suspendisse consectetur mauris vitae purus aliquet bibendum non sit amet libero. Nullam velit justo, dapibus et nunc sed, sodales fringilla ex. Donec consectetur, lacus vitae vehicula sodales, dolor arcu maximus libero, vel lacinia nunc velit sed felis. In ut mi sed tellus sagittis commodo. Sed neque ipsum, vehicula ut ipsum et, semper dictum ante. Vestibulum ante ipsum primis in faucibus orci luctus et ultrices posuere cubilia ; "
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="13dp"
        tools:layout_editor_absoluteY="147dp" />

</LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>